@model LoginForm.Models.UsersCredential

@{
    ViewData["Title"] = "Dashboard";
}

<h1>Dashboard</h1>

<div>
    <h4>UsersCredential</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class = "col-sm-10">
            @ViewBag.email
        </dd>

        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Password)
        </dt>
        <dd class = "col-sm-10">
            @ViewBag.password
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Logout">Logout</a>
</div>
